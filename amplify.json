{
  "version": 1,
  "frontend": {
    "phases": {
      "build": {
        "commands": ["npm install", "npm run build"]
      },
      "postBuild": {
        "commands": ["npm run export"]
      }
    },
    "artifacts": {
      "baseDirectory": "out",
      "files": ["**/*"]
    },
    "cache": {
      "paths": ["node_modules/**/*"]
    }
  }
}
